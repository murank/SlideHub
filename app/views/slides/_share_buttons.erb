<ul class="share-buttons">
<% if false %>
  <li><a href="https://www.facebook.com/sharer/sharer.php?u=<%= URI.encode request.url %>&t=" title="Share on Facebook" target="_blank" onclick="window.open('https://www.facebook.com/sharer/sharer.php?u=' + encodeURIComponent(document.URL) + '&t=' + encodeURIComponent(document.URL)); return false;"><img src="/oss/images/share/Facebook.png"></a></li>
  <li><a href="https://twitter.com/intent/tweet?source=<%= URI.encode request.url %>&text=:%20<%= URI.encode request.url %>" target="_blank" title="Tweet" onclick="window.open('https://twitter.com/intent/tweet?text=' + encodeURIComponent(document.title) + ':%20'  + encodeURIComponent(document.URL)); return false;"><img src="/oss/images/share/Twitter.png"></a></li>
  <li><a href="https://plus.google.com/share?url=<%= URI.encode request.url %>" target="_blank" title="Share on Google+" onclick="window.open('https://plus.google.com/share?url=' + encodeURIComponent(document.URL)); return false;"><img src="/oss/images/share/Google.png"></a></li>
  <li><a href="http://www.linkedin.com/shareArticle?mini=true&url=<%= URI.encode request.url %>&title=&summary=&source=<%= URI.encode request.url %>" target="_blank" title="Share on LinkedIn" onclick="window.open('http://www.linkedin.com/shareArticle?mini=true&url=' + encodeURIComponent(document.URL) + '&title=' +  encodeURIComponent(document.title)); return false;"><img src="/oss/images/share/LinkedIn.png"></a></li>
<% end %>
  <li><a href="mailto:?subject=&body=:%20<%= URI.encode request.url %>" target="_blank" title="Email" onclick="window.open('mailto:?subject=' + encodeURIComponent(document.title) + '&body=' +  encodeURIComponent(document.URL)); return false;"><img src="/oss/images/share/Email.png"></a></li>
</ul>
